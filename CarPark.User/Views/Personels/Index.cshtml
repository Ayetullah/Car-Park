@model IEnumerable<CarPark.Entities.Models.Personel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <td>Id</td>
        <td>Email</td>
        <td>Olusturulma Tarihi</td>
        <td>Kullanıcı Adı</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Email</td>
            <td>@item.CreatedDate</td>
            <td>@item.UserName</td>
        </tr>
    }
</table>

